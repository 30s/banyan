<?xml version="1.0" encoding="UTF-8"?>
<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">

    <!-- app -->
    <form name="findAppForm" target="findApp" title="" type="single"
          header-row-style="header-row" default-table-style="basic-table">
        <field name="name" title="${uiLabelMap.appName}"><text-find/></field>
        <field name="submitButton" title="${uiLabelMap.CommonFind}"><submit button-type="button"/></field>
    </form>
    <form name="appListForm" type="list" list-name="listIt" target="" title="" separate-columns="true" paginate-target="findApp"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar" >
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="parameters" />
                <field-map field-name="entityName" value="App"/>
                <field-map field-name="noConditionFind" value="Y"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="appId"><display /></field>
        <field name="name" title="${uiLabelMap.appName}" ><display /></field>
        <field name="fromDate"><display/></field>
        <field name="createdStamp" title="${uiLabelMap.CommonCreatedStamp}"><display /></field>
        <field name="lastUpdatedStamp" title="${uiLabelMap.CommonLastUpdatedStamp}"><display /></field>
        <field name="queues" title="${uiLabelMap.btnListQueues}" widget-style="buttontext">
            <hyperlink target="findQueue" description="${uiLabelMap.btnListQueues}" also-hidden="false">
                <parameter param-name="appId"/>
            </hyperlink>
        </field>
        <field name="update" title="${uiLabelMap.CommonUpdate}" widget-style="buttontext">
            <hyperlink target="editApp" description="${uiLabelMap.CommonUpdate}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
    </form>
    <form name="createAppForm" target="createApp" title="" type="single" header-row-style="header-row"
          default-table-style="basic-table">
        <field name="name" title="${uiLabelMap.appName}"><text /></field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="buttontext">
            <hyperlink target="${donePage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}" />
        </field>
    </form>


    <!-- queue -->
    <form name="findQueueForm" target="findQueue" title="" type="single"
          header-row-style="header-row" default-table-style="basic-table">
        <field name="name" title="${uiLabelMap.queueName}"><text-find/></field>
        <field name="isVirtual" title="${uiLabelMap.isVirtual}">
            <radio no-current-selected-key="">
                <option key="0" description="否" />
                <option key="1" description="是" />
            </radio>
        </field>
        <field name="appId" title="${uiLabelMap.App}">
            <drop-down allow-empty="true">
                <entity-options entity-name="App" description="${name}" />
            </drop-down>
        </field>
        <field name="communicateType" title="${uiLabelMap.communicateType}">
            <drop-down allow-empty="true">
                <option key="produce" description="produce" />
                <option key="consume" description="consume" />
                <option key="produce-consume" description="produce-consume" />
                <option key="publish" description="publish" />
                <option key="subscribe" description="subscribe" />
                <option key="publish-subscribe" description="publish-subscribe" />
                <option key="request" description="request" />
                <option key="response" description="response" />
                <option key="request-response" description="request-response" />
                <option key="rpcrequest" description="rpcrequest" />
                <option key="rpcresponse" description="rpcresponse" />
                <option key="rpcrequest-rpcresponse" description="rpcrequest-rpcresponse" />
            </drop-down>
        </field>
        <field name="auditTypeCode" title="${uiLabelMap.auditTypeCode}">
            <drop-down allow-empty="true">
                <option key="AUDIT_BEFORE" description="未审核" />
                <option key="AUDIT_SUCCESS" description="审核通过" />
                <option key="AUDIT_FAILED" description="审核失败" />
            </drop-down>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonFind}"><submit button-type="button"/></field>
    </form>
    <form name="queueListForm" type="list" list-name="listIt" target="" title="" separate-columns="true" paginate-target="findQueue"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar" >
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="requestParameters"/>
                <field-map field-name="entityName" value="Node"/>
                <field-map field-name="noConditionFind" value="Y"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="name" title="${uiLabelMap.NodeName}" ><display /></field>
        <!--<field name="value" title="${uiLabelMap.NodeValue}" ><display /></field>-->
        <field name="communicateType" title="${uiLabelMap.communicateType}"><display /></field>
        <field name="isVirtual" title="${uiLabelMap.isVirtual}"><display description="${(isVirtual==1)?'是':'否'}" /></field>
        <field name="rateLimit" title="${uiLabelMap.rateLimit}"><display /></field>
        <field name="threshold" title="${uiLabelMap.threshold}"><display /></field>
        <field name="msgBodySize" title="${uiLabelMap.msgBodySize}"><display /></field>
        <field name="ttl" title="${uiLabelMap.ttl}"><display /></field>
        <field name="ttlPerMsg" title="${uiLabelMap.ttlPerMsg}"><display /></field>
        <field name="appId" title="${uiLabelMap.App}"><display-entity entity-name="App" description="${name}" /> </field>
        <field name="auditTypeCode" title="${uiLabelMap.auditTypeCode}"><display-entity entity-name="AuditType" description="${auditTypeName}" /></field>
        <field name="rateWarning" title="${uiLabelMap.rateWarning}" widget-style="buttontext">
            <hyperlink target="queueRateWarning" description="${uiLabelMap.rateWarning}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
        <field name="detail" title="${uiLabelMap.CommonDetails}" widget-style="buttontext">
            <hyperlink target="queueDetail" description="${uiLabelMap.CommonDetails}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
        <field name="update" title="${uiLabelMap.CommonUpdate}" widget-style="buttontext">
            <hyperlink target="editQueue" description="${uiLabelMap.CommonUpdate}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
        <field name="remove" title="${uiLabelMap.CommonRemove}" widget-style="buttontext">
            <hyperlink target="removeQueue" description="${uiLabelMap.CommonRemove}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
    </form>
    <form name="createQueueForm" target="createQueue" title="" type="single" header-row-style="header-row"
          default-table-style="basic-table">
        <field name="name" title="${uiLabelMap.NodeName}"><text /></field>
        <field name="description" title="${uiLabelMap.description}"><text /></field>
        <field name="communicateType" title="${uiLabelMap.communicateType}">
            <drop-down allow-empty="false">
                <option key="_NA_" description="请选择通信类型" />
                <option key="produce" description="produce" />
                <option key="consume" description="consume" />
                <option key="produce-consume" description="produce-consume" />
                <option key="publish" description="publish" />
                <option key="subscribe" description="subscribe" />
                <option key="publish-subscribe" description="publish-subscribe" />
                <option key="request" description="request" />
                <option key="response" description="response" />
                <option key="request-response" description="request-response" />
                <option key="rpcrequest" description="rpcrequest" />
                <option key="rpcresponse" description="rpcresponse" />
                <option key="rpcrequest-rpcresponse" description="rpcrequest-rpcresponse" />
            </drop-down>
        </field>
        <field name="appId" title="${uiLabelMap.appName}">
            <drop-down allow-empty="false">
                <list-options list-name="currentUserApps" key-name="appId" description="${name}" />
            </drop-down>
        </field>
        <field name="rateLimit" title="${uiLabelMap.rateLimit}"><text /></field>
        <field name="threshold" title="${uiLabelMap.threshold}"><text /></field>
        <field name="msgBodySize" title="${uiLabelMap.msgBodySize}"><text /></field>
        <field name="ttl" title="${uiLabelMap.ttl}"><text /></field>
        <field name="ttlPerMsg" title="${uiLabelMap.ttlPerMsg}"><text /></field>
        <field name="canBroadcast" title="${uiLabelMap.canBroadcast}">
            <radio>
                <option key="0" description="否" />
                <option key="1" description="是" />
            </radio>
        </field>
        <field name="compress" title="${uiLabelMap.compress}">
            <radio>
                <option key="" description="否" />
                <option key="snappy" description="snappy" />
                <option key="lzf" description="lzf" />
            </radio>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="buttontext">
            <hyperlink target="${donePage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}" />
        </field>
    </form>
    <form name="updateQueueForm" target="updateQueue" title="" type="single"
          header-row-style="header-row" default-table-style="basic-table" default-map-name="editingQueue">
        <field name="nodeId"><hidden /></field>
        <field name="name" title="${uiLabelMap.NodeName}"><text /></field>
        <field name="description" title="${uiLabelMap.description}"><text /></field>
        <field name="available" title="${uiLabelMap.available}">
            <radio>
                <option key="0" description="否" />
                <option key="1" description="是" />
            </radio>
        </field>
        <field name="canBroadcast" title="${uiLabelMap.canBroadcast}">
            <radio>
                <option key="0" description="否" />
                <option key="1" description="是" />
            </radio>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="smallSubmit">
            <hyperlink target="${cancelPage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}" />
        </field>
    </form>
    <form name="queueDetailForm" type="single" default-map-name="lookupNode">
        <field name="name" title="${uiLabelMap.NodeName}"><display /></field>
        <field name="type" title="${uiLabelMap.nodeType}"><display description="${(lookupNode.type==0)?'exchange':'queue'}" /></field>
        <field name="communicateType" title="${uiLabelMap.communicateType}"><display /></field>
        <field name="value" title="${uiLabelMap.NodeValue}"><display/></field>
        <field name="routerType" title="${uiLabelMap.routerType}"><display /></field>
        <field name="routingKey" title="${uiLabelMap.routingKey}"><display /></field>
        <field name="rateLimit" title="${uiLabelMap.rateLimit}"><display /></field>
        <field name="threshold" title="${uiLabelMap.threshold}"><display /></field>
        <field name="msgBodySize" title="${uiLabelMap.msgBodySize}"><display /></field>
        <field name="auditTypeCode" title="${uiLabelMap.auditTypeCode}">
            <display-entity entity-name="AuditType" description="${auditTypeName}" />
        </field>
        <field name="creator" title="${uiLabelMap.creator}" entry-name="creator">
            <display-entity entity-name="UserLogin" key-field-name="userLoginId" description="${userLoginId} ${firstName} ${lastName}"/>
        </field>


        <!--<field name="statusId"><display-entity entity-name="StatusItem" key-field-name="statusId" description="${description}"/></field>-->
    </form>
    <form name="queueRateWarningListForm" type="list" list-name="listIt" target="" title="" separate-columns="true" paginate-target="queueRateWarning"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar" >
        <actions>
            <service service-name="getQueueRateWarningListService" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="requestParameters"/>
                <field-map field-name="entityName" value="QueueRateWarning"/>
                <field-map field-name="noConditionFind" value="N"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="nodeId" title="${uiLabelMap.NodeName}" ><display-entity entity-name="Node" description="${name}" /></field>
        <field name="rateLimit" title="${uiLabelMap.maxRate}"><display /></field>
        <field name="realRate" title="${uiLabelMap.realRate}"><display /></field>
        <field name="fromDate" title="${uiLabelMap.rateWarningDate}"><display /></field>
    </form>

    <!-- config -->
    <form name="findConfigItem" target="findConfigItem" title="" type="single"
          header-row-style="header-row" default-table-style="basic-table">
        <field name="itemKey" title="${uiLabelMap.itemKey}"><text-find/></field>
        <field name="submitButton" title="${uiLabelMap.CommonFind}"><submit button-type="button"/></field>
    </form>
    <form name="configItemList" type="list" list-name="listIt" target="" title="" separate-columns="true" paginate="true" paginate-target="findConfig"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar">
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="requestParameters"/>
                <field-map field-name="entityName" value="Config"/>
                <field-map field-name="noConditionFind" value="Y"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="itemKey" title="${uiLabelMap.itemKey}" ><display /></field>
        <field name="itemValue" title="${uiLabelMap.itemValue}" ><display /></field>
        <field name="type" title="${uiLabelMap.itemType}"><display /></field>
        <field name="update" title="${uiLabelMap.CommonUpdate}" widget-style="buttontext">
            <hyperlink target="editConfigItem" description="${uiLabelMap.CommonUpdate}" also-hidden="false">
                <parameter param-name="configId"/>
            </hyperlink>
        </field>
        <field name="remove" title="${uiLabelMap.CommonRemove}" widget-style="buttontext">
            <hyperlink target="removeConfigItem" description="${uiLabelMap.CommonRemove}" also-hidden="false">
                <parameter param-name="configId"/>
            </hyperlink>
        </field>
    </form>
    <form name="addConfigItemForm" target="createConfigItem" title="" type="single" header-row-style="header-row"
          default-table-style="basic-table">
        <field name="itemKey" title="${uiLabelMap.itemKey}"><text /></field>
        <field name="itemValue" title="${uiLabelMap.itemValue}"><text /></field>
        <field name="type" title="${uiLabelMap.itemType}">
            <drop-down allow-empty="false">
                <option key="_NA_" description="请选择配置项类型" />
                <option key="client" description="client" />
                <option key="server" description="server" />
                <option key="rabbitMQServer" description="rabbitMQServer" />
            </drop-down>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="buttontext">
            <hyperlink target="${donePage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}" />
        </field>
    </form>
    <form name="editConfigItemForm" target="updateConfigItem" title="" type="single" header-row-style="header-row"
          default-table-style="basic-table" default-map-name="editingConfigItem">
        <field name="configId"><hidden /></field>
        <field name="itemKey" title="${uiLabelMap.itemKey}"><text /></field>
        <field name="itemValue" title="${uiLabelMap.itemValue}"><text /></field>
        <field name="type" title="${uiLabelMap.type}"><text /></field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="smallSubmit">
            <hyperlink target="${cancelPage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}" />
        </field>
    </form>

    <!-- sink -->
    <form name="findSinkForm" target="findSink" title="" type="single"
          header-row-style="header-row" default-table-style="basic-table">
        <field name="flowFrom" title="${uiLabelMap.flowFrom}">
            <lookup target-form-name="LookupQueueName" />
        </field>
        <field name="flowTo" title="${uiLabelMap.flowTo}">
            <lookup target-form-name="LookupQueueName" />
        </field>
        <field name="enable" title="${uiLabelMap.CommonEnable}">
            <radio>
                <option key="1" description="${uiLabelMap.enable}" />
                <option key="0" description="${uiLabelMap.disable}" />
            </radio>
        </field>
        <field name="auditTypeCode" title="${uiLabelMap.auditTypeCode}">
            <drop-down allow-empty="true">
                <option key="AUDIT_BEFORE" description="未审核" />
                <option key="AUDIT_SUCCESS" description="审核通过" />
                <option key="AUDIT_FAILED" description="审核失败" />
            </drop-down>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonFind}"><submit button-type="button"/></field>
    </form>
    <form name="sinkListForm" type="list" list-name="listIt" target="" title="" separate-columns="true" paginate-target="findSink"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar">
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <!--<field-map field-name="inputFields" from-field="parameters"/>-->
                <field-map field-name="inputFields" from-field="requestParameters"/>
                <field-map field-name="entityName" value="SinkAndNode"/>
                <field-map field-name="noConditionFind" value="Y"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="token" title="${uiLabelMap.token}"><display /></field>
        <field name="flowFromNodeName" title="${uiLabelMap.flowFrom}" >
            <hyperlink target="queueDetail" description="${flowFromNodeName}" also-hidden="false">
                <parameter param-name="nodeId" from-field="flowFrom"/>
            </hyperlink>
        </field>
        <field name="fromCommunicateType" title="${uiLabelMap.fromCommunicateType}"><display /></field>
        <field name="flowToNodeName" title="${uiLabelMap.flowTo}" >
            <hyperlink target="queueDetail" description="${flowToNodeName}" also-hidden="false">
                <parameter param-name="nodeId" from-field="flowTo" />
            </hyperlink>
        </field>
        <field name="toCommunicateType" title="${uiLabelMap.toCommunicateType}"><display /></field>
        <field name="auditTypeCode" title="${uiLabelMap.auditType}">
            <display-entity entity-name="AuditType" description="${auditTypeName}" />
        </field>
        <field name="remove" title="${uiLabelMap.CommonRemove}" widget-style="buttontext">
            <hyperlink target="removeSink" description="${uiLabelMap.CommonRemove}" also-hidden="false">
                <parameter param-name="sinkId"/>
            </hyperlink>
        </field>
    </form>
    <form name="createSinkForm" target="createSink" title="" type="single" header-row-style="header-row"
          default-table-style="basic-table">
        <field name="flowFrom" title="${uiLabelMap.flowFrom}"><lookup target-form-name="LookupQueueName" /></field>
        <field name="flowTo" title="${uiLabelMap.flowTo}"><lookup target-form-name="LookupQueueName" /></field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="buttontext">
            <hyperlink target="${donePage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}" />
        </field>
    </form>

    <!-- maintain (node) -->
    <form name="nodeListForm" type="list" list-name="listIt" target="" title="" separate-columns="true" paginate-target="findApp"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar" >
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="requestParameters"/>
                <field-map field-name="entityName" value="Node"/>
                <field-map field-name="noConditionFind" value="Y"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="name" title="${uiLabelMap.NodeName}" ><display /></field>
        <field name="value" title="${uiLabelMap.NodeValue}" ><display /></field>
        <field name="routerType" title="${uiLabelMap.routerType}"><display /></field>
        <field name="routingKey" title="${uiLabelMap.routingKey}"><display /></field>
        <field name="detail" title="${uiLabelMap.CommonDetails}" widget-style="buttontext">
            <hyperlink target="queueDetail" description="${uiLabelMap.CommonDetails}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
        <field name="update" title="${uiLabelMap.CommonUpdate}" widget-style="buttontext">
            <hyperlink target="editApp" description="${uiLabelMap.CommonUpdate}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
        <field name="remove" title="${uiLabelMap.CommonRemove}" widget-style="buttontext">
            <hyperlink target="removeNode" description="${uiLabelMap.CommonRemove}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
    </form>
    <form name="findNodeForm" target="findNode" type="single" header-row-style="header-row" default-table-style="basic-table" >
        <field name="name" title="${uiLabelMap.NodeName}"><text-find/></field>
        <field name="submitButton" title="${uiLabelMap.CommonFind}"><submit button-type="button"/></field>
    </form>
    <form name="editNodeForm" target="updateNode" title="" type="single" header-row-style="header-row"
          default-table-style="basic-table" default-map-name="editingNode">
        <field name="name" title="${uiLabelMap.NodeName}"><text /></field>
        <field name="nodeId"><hidden /></field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit /></field>
        <field name="cancelLink" title="" widget-style="smallSubmit">
            <hyperlink target="${cancelPage}" also-hidden="false" description="${uiLabelMap.CommonCancelDone}">
                <parameter param-name="nodeId" />
            </hyperlink>
        </field>
    </form>
    <form name="ViewNodeDetail" type="single" default-map-name="lookupNode">
        <field name="name" title="${uiLabelMap.NodeName}"><display /></field>
        <field name="description" title="${uiLabelMap.description}"><display /></field>
        <field name="secret" title="${uiLabelMap.secret}"><display /></field>
        <field name="value" title="${uiLabelMap.NodeValue}"><display/></field>
        <field name="routerType" title="${uiLabelMap.routerType}"><display /></field>
        <field name="routingKey" title="${uiLabelMap.routingKey}"><display /></field>
        <field name="type" title="${uiLabelMap.nodeType}"><display description="${(lookupNode.type==0)?'exchange':'queue'}" /></field>
        <field name="canBroadcast" title="${uiLabelMap.canBroadcast}"><display description="${(lookupNode.canBroadcast==0)?'否':'是'}" /></field>
        <field name="compress" title="${uiLabelMap.compress}"><display /></field>
        <!--<field name="statusId"><display-entity entity-name="StatusItem" key-field-name="statusId" description="${description}"/></field>-->
    </form>

    <!-- maintain (audit) -->
    <form name="unAuditedQListSnapshotForm" type="list" paginate="false" list-name="listIt" target="" title="" separate-columns="true" paginate-target="#"
          odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar" >
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="requestParameters" />
                <field-map field-name="entityName" value="Node"/>
                <field-map field-name="noConditionFind" value="Y"/>
                <field-map field-name="viewIndex" from-field="viewIndex"/>
                <field-map field-name="viewSize" from-field="viewSize"/>
            </service>
        </actions>
        <field name="name" title="${uiLabelMap.NodeName}" >
            <hyperlink target="queueDetail" description="${name}" also-hidden="false">
                <parameter param-name="nodeId"/>
            </hyperlink>
        </field>
        <field name="communicateType" title="${uiLabelMap.communicateType}"><display /></field>
        <field name="audit" title="${uiLabelMap.CommonAudit}" widget-style="buttontext">
            <hyperlink target="auditQueue" description="${uiLabelMap.CommonAudit}" also-hidden="false">
                <parameter from-field="nodeId" param-name="queueId" />
            </hyperlink>
