                <set field="viewSize" from-field="parameters.VIEW_SIZE" type="Integer" default-value="10"/>
                <set field="requestParameters.creator" value="${userLogin.userLoginId}"/>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <section>
                            <widgets>
                                <decorator-screen name="FindScreenDecorator"
                                                  location="component://common/widget/CommonScreens.xml">
                                    <decorator-section name="menu-bar">
                                        <container style="button-bar">
                                            <link target="addSink" style="buttontext create"
                                                  text="${uiLabelMap.addSink}"/>
                                        </container>
                                    </decorator-section>
                                    <decorator-section name="search-options">
                                        <include-form name="findSinkForm"
                                                      location="component://banyan/widget/BanyanForms.xml"/>
                                    </decorator-section>
                                    <decorator-section name="search-results">
                                        <include-form name="sinkListForm"
                                                      location="component://banyan/widget/BanyanForms.xml"/>
                                    </decorator-section>
                                </decorator-screen>
                            </widgets>
                            <fail-widgets>
                                <label style="h3">Error.</label>
                            </fail-widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="addSinkScreen">
        <section>
            <actions>
                <set field="headerItem" value="sink"/>
                <set field="titleProperty" value="PageTitleAddSink"/>
                <set field="donePage" from-field="parameters.DONE_PAGE" default-value="main"/>
                <entity-condition entity-name="Node" list="availableQueues">
                    <condition-list>
                        <condition-expr field-name="available" operator="equals" value="1"/>
                        <condition-expr field-name="isInner" operator="equals" value="0"/>
                        <condition-expr field-name="auditTypeCode" operator="equals" value="AUDIT_SUCCESS"/>
                        <condition-expr field-name="type" operator="equals" value="1"/>
                    </condition-list>
                </entity-condition>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <container style="h1">
                            <label text="${uiLabelMap.PageTitleAddSink}"/>
                        </container>
                        <include-form name="createSinkForm"
                                      location="component://banyan/widget/BanyanForms.xml"/>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>

