                                              location="component://banyan/widget/BanyanMenus.xml"/>
                                <include-form name="unAuditedSinkListForm"
                                              location="component://banyan/widget/BanyanForms.xml"/>
                            </widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>

</screens>