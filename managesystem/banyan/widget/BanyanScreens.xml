<?xml version="1.0" encoding="UTF-8"?>
<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="https://ofbiz.apache.org/dtds/widget-screen.xsd">

    <screen name="main">
        <section>
            <actions>
                <set field="headerItem" value="main"/>
                <set field="titleProperty" value="PageTitleViewAppList"/>
                <set field="viewIndex" from-field="parameters.VIEW_INDEX" type="Integer" default-value="0"/>
                <set field="viewSize" from-field="parameters.VIEW_SIZE" type="Integer" default-value="10"/>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <section>
                            <widgets>
                                <include-portal-page id="banyanDashboard"/>
                            </widgets>
                            <fail-widgets>
                                <label style="h3">Error.</label>
                            </fail-widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="mqServerOverviewScreen">
        <section>
            <widgets>
                <platform-specific>
                    <html>
                        <html-template location="component://banyan/webapp/banyan/ftl/mqServerOverview.ftl"/>
                    </html>
                </platform-specific>
            </widgets>
        </section>
    </screen>
    <screen name="queuedMsgChartScreen">
        <section>
            <widgets>
                <platform-specific>
                    <html>
                        <html-template location="component://banyan/webapp/banyan/ftl/queuedMsgChart.ftl"/>
                    </html>
                </platform-specific>
            </widgets>
        </section>
    </screen>
    <screen name="msgRateChartScreen">
        <section>
            <widgets>
                <platform-specific>
                    <html>
                        <html-template location="component://banyan/webapp/banyan/ftl/msgRateChart.ftl"/>
                    </html>
                </platform-specific>
            </widgets>
        </section>
    </screen>
    <screen name="queuedMsgRateChartScreen">
        <section>
            <widgets>
                <platform-specific>
                    <html>
                        <html-template location="component://banyan/webapp/banyan/ftl/queuedMsgRateChart.ftl"/>
                    </html>
                </platform-specific>
            </widgets>
        </section>
    </screen>

    <!-- queue -->
    <screen name="queueScreen">
        <section>
            <actions>
                <set field="headerItem" value="queue"/>
                <set field="titleProperty" value="PageTitleViewQueueList"/>
                <set field="viewIndex" from-field="parameters.VIEW_INDEX" type="Integer" default-value="0"/>
                <set field="viewSize" from-field="parameters.VIEW_SIZE" type="Integer" default-value="10"/>
                <set field="requestParameters.isInner" value="0"/>
                <set field="requestParameters.type" value="1"/>
                <set field="requestParameters.creator" value="${userLogin.userLoginId}"/>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <section>
                            <widgets>
                                <decorator-screen name="FindScreenDecorator"
                                                  location="component://common/widget/CommonScreens.xml">
                                    <decorator-section name="menu-bar">
                                        <container style="button-bar">
                                            <link target="addQueue" style="buttontext create"
                                                  text="${uiLabelMap.addQueue}"/>
                                        </container>
                                    </decorator-section>
                                    <decorator-section name="search-options">
                                        <include-form name="findQueueForm"
                                                      location="component://banyan/widget/BanyanForms.xml"/>
                                    </decorator-section>
                                    <decorator-section name="search-results">
                                        <include-form name="queueListForm"
                                                      location="component://banyan/widget/BanyanForms.xml"/>
                                    </decorator-section>
                                </decorator-screen>
                            </widgets>
                            <fail-widgets>
                                <label style="h3">Error.</label>
                            </fail-widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="addQueueScreen">
        <section>
            <actions>
                <set field="headerItem" value="queue"/>
                <set field="titleProperty" value="PageTitleAddQueue"/>
                <set field="donePage" from-field="parameters.DONE_PAGE" default-value="main"/>
                <entity-condition entity-name="App" list="currentUserApps">
                    <condition-list>
                        <condition-expr field-name="creator" operator="equals" value="${userLogin.userLoginId}"/>
                    </condition-list>
                </entity-condition>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <container style="h1">
                            <label text="${uiLabelMap.PageTitleAddQueue}"/>
                        </container>
                        <include-form name="createQueueForm"
                                      location="component://banyan/widget/BanyanForms.xml"/>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="editQueueScreen">
        <section>
            <actions>
                <set field="headerItem" value="queue"/>
                <set field="cancelPage" from-field="parameters.CANCEL_PAGE" default-value="queue"/>
                <set field="nodeId" from-field="parameters.nodeId"/>
                <entity-one entity-name="Node" value-field="editingQueue"/>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <container style="h1">
                            <label text="${uiLabelMap.PageTitleEditQueue}"/>
                        </container>
                        <include-form name="updateQueueForm" location="component://banyan/widget/BanyanForms.xml"/>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="queueDetailScreen">
        <section>
            <actions>
                <set field="headerItem" value="queue"/>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <section>
                            <widgets>
                                <container id="split50">
                                    <container style="lefthalf">
                                        <include-screen name="queueInfo"/>
                                    </container>
                                    <container style="righthalf">
                                    </container>
                                </container>
                            </widgets>
                            <fail-widgets>
                                <label style="h2" text="error"/>
                            </fail-widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="queueInfo">
        <section>
            <actions>
                <entity-one entity-name="Node" value-field="lookupNode"/>
            </actions>
            <widgets>
                <screenlet title="${uiLabelMap.NodeInformation}" navigation-menu-name="MenuQueueUpdate"
                           id="NodeInformationPanel">
                    <include-menu name="MenuQueueUpdate" location="component://banyan/widget/BanyanMenus.xml"/>
                    <include-form name="queueDetailForm" location="component://banyan/widget/BanyanForms.xml"/>
                </screenlet>
            </widgets>
        </section>
    </screen>
    <screen name="queueRateWarningScreen">
        <section>
            <actions>
                <set field="headerItem" value="queue"/>
                <set field="titleProperty" value="PageTitleViewQueueList"/>
                <set field="viewIndex" from-field="parameters.VIEW_INDEX" type="Integer" default-value="0"/>
                <set field="viewSize" from-field="parameters.VIEW_SIZE" type="Integer" default-value="10"/>
            </actions>
            <widgets>
                <decorator-screen name="BanyanCommonDecorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <section>
                            <widgets>
                                <include-form name="queueRateWarningListForm"
                                              location="component://banyan/widget/BanyanForms.xml"/>
                            </widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>

