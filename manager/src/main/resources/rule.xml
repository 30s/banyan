<?xml version="1.0" encoding="UTF-8" ?>
<rules xmlns="http://com.freedom.messagebus">
    <routingKeys>
        <rule>
            <rule-name>system</rule-name>
            <rule-pattern>routingkey.message.sys.</rule-pattern>
        </rule>
        <rule>
            <rule-name>business</rule-name>
            <rule-pattern>routingkey.message.business.</rule-pattern>
        </rule>
    </routingKeys>
    <queueNames>
        <rule>
            <rule-name>system</rule-name>
            <rule-pattern>queue.proxy.message.sys.</rule-pattern>
        </rule>
        <rule>
            <rule-name>business</rule-name>
            <rule-pattern>queue.proxy.message.business.</rule-pattern>
        </rule>
    </queueNames>
</rules>